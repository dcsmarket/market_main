        <record id="ir_cron_action_auto_invoice" model="ir.cron">
            <field name="name">Make Auto Invoice</field>
            <field name="model_id" ref="point_of_sale.model_pos_order"/>
            <field name="state">code</field>
            <field name="code">model._run_auto_invoice()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
        </record>


        <!-- Send Edi Error Schedule Action -->
        <record id="ir_cron_action_electronic_invoice" model="ir.cron">
            <field name="name">Edi Send Error</field>
            <field name="model_id" ref="account.model_account_move"/>
            <field name="state">code</field>
            <field name="code">model._run_edi_error_invoice()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="priority">5</field>
        </record>
