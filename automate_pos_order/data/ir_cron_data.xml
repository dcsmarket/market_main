            <field name="name">EDI: Perform web services operations Test</field>
            <field name="model_id" ref="account_edi.model_account_edi_document"/>
            <field name="state">code</field>
            <field name="code">model._cron_process_documents_web_services(job_count=350)</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">25</field>
            <field name="interval_type">minutes</field>
